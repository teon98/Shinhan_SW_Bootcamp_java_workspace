package com.shinhan.day02;

import java.util.Scanner;

public class 조건문 {

	public static void main(String[] args) {
		method1();

	}

	private static void method1() {
		Scanner sc = new Scanner(System.in);
		System.out.print("점수>>");
		int score = sc.nextInt();
		String grade;
		if(score >= 90) {
			grade = "A";
		}else {
			grade="";
		}
		System.out.println("결과는 " + score + "==>"+ grade);
	}

}
