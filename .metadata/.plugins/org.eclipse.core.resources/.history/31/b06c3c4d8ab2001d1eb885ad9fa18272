package com.shinhan.day02;

public class 태영 {

	public static void main(String[] args) {
		//method1();
		method2();
	}

	private static void method2() {
		int score = 85;
		String result = ((!(score>90)))?"가":"나";
		System.out.println(result);
	}

	private static void method1() {
		int x = 10;
		int y = 20;
		int z = (++x) + (y--);
		System.out.println(z); //정답:31
	}

}
