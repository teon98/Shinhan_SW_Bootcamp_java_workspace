package com.shinhan.day03;

public class 연습문제5장 {

	public static void main(String[] args) {
		method7();
	}

	private static void method7() {
		int [] array = {1,5,3,8,2};
		
		int max = 0;
		
		for(int i=0; i<array.length; i++) {
			if(array[i] > max) {
				max = array[i]
			}
		}
		
		System.out.printf("최댓값은 %d",max);
		
	}

}
