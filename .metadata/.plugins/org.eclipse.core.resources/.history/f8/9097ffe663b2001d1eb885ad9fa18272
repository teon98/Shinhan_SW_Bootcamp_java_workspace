package com.shinhan.day02;

import java.util.Scanner;

public class LAB5 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("직각 역삼각형을 출력할 줄 수 : ");
		int line = sc.nextInt();
		
		for(int j=0; j<10; j++) {
			for(int i=line; i>0; i--) {
				System.out.print("*");
			}
			System.out.println("");
		}
	}

}
